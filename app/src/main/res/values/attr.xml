<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SDTextView">
        <attr name="customTypeface" format="string" />
    </declare-styleable>

    <declare-styleable name="ExpandableLayout">
        <attr name="el_duration" format="integer" />
        <attr name="el_expanded" format="boolean" />
        <attr name="el_parallax" format="float" />
        <attr name="android:orientation"/>
    </declare-styleable>

    <declare-styleable name="easy_flip_view">
        <attr name="flipOnTouch" format="boolean"/>
        <attr name="flipDuration" format="integer"/>
        <attr name="flipType" format="string"/>
        <attr name="flipFrom" format="string"/>
        <attr name="flipEnabled" format="boolean"/>
        <attr name="animFlipInId" format="reference"/>
        <attr name="animFlipOutId" format="reference"/>
    </declare-styleable>

    <declare-styleable name="RecyclerViewPager">
        <attr name="rvp_triggerOffset" format="float" />
        <attr name="rvp_flingFactor" format="float" />
        <attr name="rvp_singlePageFling" format="boolean" />
        <attr name="rvp_inertia" format="boolean"/>
        <attr name="rvp_millisecondsPerInch" format="float" />
    </declare-styleable>

    <declare-styleable name="MetalRecyclerViewPager">
        <attr name="itemMargin" format="dimension"/>
    </declare-styleable>

</resources>